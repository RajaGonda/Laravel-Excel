{
  "name": "mirrorwebs/excel",
  "description": "Supercharged Excel exports and imports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "mirrorwebs",
      "email": "mirrorwebs@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.3|^7.4|^7.5",
    "phpoffice/phpspreadsheet": "^1.14",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.4|^9.0",
    "mockery/mockery": "^1.4",
    "orchestra/database": "^5.1",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Rajagonda\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rajagonda\\Excel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rajagonda\\Excel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "Rajagonda\\Excel\\Facades\\Excel"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
